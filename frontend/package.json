    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "imports-loader": "^0.8.0",
    "roboto-fontface": "*",
    "vee-validate": "^3.0.11",
    "vue": "^2.6.10",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.3",
    "vue-the-mask": "^0.11.1",
    "vuelidate": "^0.7.4",
    "vuetify": "^2.0.0",
    "vulidate": "^0.2.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
